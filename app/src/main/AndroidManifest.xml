<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.stan.androidgroupproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Automobile"
            android:label="@string/automobile"
            android:parentActivityName=".MainActivity"
            >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"
                />
        </activity>

        <activity android:name=".RefuelActivity"
            android:parentActivityName=".Automobile">

            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Automobile"
                />
        </activity>

        <activity android:name=".NutriMain">

    </activity>
        <activity android:name=".CalculateCalories"></activity>
        <activity android:name=".EditFood"></activity>
        <activity android:name=".EmptyFrameLayout"></activity>
        <activity android:name=".FoodChat"></activity>
        <activity android:name=".FoodInfoForm"></activity>
        <activity android:name=".Transition"></activity>

    </application>
</manifest>
